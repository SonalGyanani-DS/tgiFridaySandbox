{
  "$id": "c_Review_Keyword_Sentiment125924",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Review Keyword Sentiment",
  "description": "Keyword Mentions, Keyword Sentiment, Average Rating, and Reviews by Review Keywords",
  "query": {
    "metrics": [
      "yext/TOPIC_MENTIONS",
      "yext/TOPIC_SENTIMENT",
      "yext/AVERAGE_RATING",
      "yext/NEW_REVIEWS"
    ],
    "dimensions": [
      "yext/REVIEW_TOPICS"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "WEEKS_12"
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/TOPIC_MENTIONS"
        },
        "descending": true,
        "rank": true
      }
    }
  }
}