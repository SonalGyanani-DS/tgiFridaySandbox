{
  "$schema": "https://schema.yext.com/config/answers/answers-config/v1",
  "$id": "tgi-fridays-search",
  "name": "TGI-Fridays Search",
  "supportedLocales": [],
  "countryRestrictions": [],
  "verticals": {
    "locations": {
      "entityTypes": [
        "location"
      ],
      "name": "Locations",
      "searchableFields": {
        "address.city": {
          "nlpFilter": true,
          "textSearch": true
        },
        "address.countryCode": {
          "nlpFilter": true,
          "textSearch": true
        },
        "address.line1": {
          "nlpFilter": true,
          "textSearch": true
        },
        "address.line2": {},
        "address.postalCode": {
          "nlpFilter": true,
          "phraseMatch": true
        },
        "address.region": {
          "nlpFilter": true,
          "textSearch": true
        },
        "address.sublocality": {},
        "builtin.entityType": {
          "nlpFilter": true
        },
        "builtin.hours": {
          "nlpFilter": true
        },
        "builtin.location": {
          "nlpFilter": true
        },
        "keywords": {
          "phraseMatch": true
        },
        "name": {
          "textSearch": true
        }
      },
      "sortBys": [
        {
          "buckets": [],
          "type": "ENTITY_DISTANCE"
        }
      ],
      "source": "YEXT"
    }
  },
  "localizations": {},
  "additionalSearchablePlaces": [],
  "querySuggestions": {
    "disablePopularQueries": false,
    "verticalPromptsOnEmpty": false,
    "popularQueriesBlacklistedTerms": [],
    "popularQueriesBlacklistedRegex": [],
    "universalPrompts": [],
    "verticalPrompts": {
      "locations": [
        "[[name]]"
      ]
    }
  }
}